package streamAPI;

public class Produtor {

	final String nome;
	final double preco;
	final double desconto;
	final boolean frete;
	
	public Produtor(String nome, double preco, double desconto, boolean frete) {
		super();
		this.nome = nome;
		this.preco = preco;
		this.desconto = desconto;
		this.frete = frete;
	}
	
}
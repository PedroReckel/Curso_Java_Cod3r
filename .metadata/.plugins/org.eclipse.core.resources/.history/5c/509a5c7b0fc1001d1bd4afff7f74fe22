package oo.heranca.desafio01;

public class Mazda extends Carro {

	Mazda(int velocidadeMaxima) {
		super(velocidadeMaxima);
	}
	
	@Override
	void acelerar() {
		velocidadeAtual += 15;
	}
	
	@Override
	void frear() {
		if (velocidadeAtual >= 10) {
			velocidadeAtual -= 10;
		} else {
			velocidadeAtual = 0;
		}
	}
	
}

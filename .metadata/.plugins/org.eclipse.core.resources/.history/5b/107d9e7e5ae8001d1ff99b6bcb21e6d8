package streamAPI;

import java.util.function.UnaryOperator;

public interface Utilitarios {

	public static UnaryOperator<String> maiuscula = n -> n.toUpperCase();
	public static UnaryOperator<String> primeiraLentra = n -> n.charAt(0) + "";
	public static UnaryOperator<String> grito() {
		return n -> n + "!!!";
	}
	
}
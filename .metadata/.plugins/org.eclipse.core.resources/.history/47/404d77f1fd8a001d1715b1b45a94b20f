package controle;

public class For1 {

	public static void main(String[] args) {
		
		for (int c = 1; c <= 20; c++ ) {
			System.out.printf("i = %d\n", c);
		}
		
	    int x = 2;
	    for (;x < 10;) {
	    	System.out.printf("j = %d\n", x);
	    	x++;
	    }
	    
	    for (;;) {
	    	System.out.println("Fim!");
	    }
		
	}
	
}

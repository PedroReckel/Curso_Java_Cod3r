package oo.heranca.desafio01;

public class Ferrari extends Carro {
	
	boolean acelerar() {
		boolean acelerou1 = super.acelerar();
		boolean acelerou2 = super.acelerar();
		boolean acelerou3 = super.acelerar();
		boolean acelerou4 = super.acelerar();
		boolean acelerou5 = super.acelerar();
		boolean freiou1 = super.frear();
		boolean freiou2 = super.frear();
		
		return acelerou1 || acelerou2 || acelerou3 || acelerou4 || acelerou5 || freiou1 || freiou2;
	}
}
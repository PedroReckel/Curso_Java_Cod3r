package oo.heranca.desafio01;

public class Ferrari extends Carro {
	
	Ferrari(int velocidadeMaxima) {
		super(velocidadeMaxima);
	}
	
	@Override
	void acelerar() {
		velocidadeAtual += 50;
	}
	
	@Override
	void frear() {
		if (velocidadeAtual >= 35) {
			velocidadeAtual -= 35;
		} else {
			velocidadeAtual = 0;
		}
	}
}
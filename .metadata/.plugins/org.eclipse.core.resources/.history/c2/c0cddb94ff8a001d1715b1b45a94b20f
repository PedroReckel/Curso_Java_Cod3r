package controle;

import java.util.Scanner;

public class WhileIndeterminado {

	public static void main(String[] args) {
		
		Scanner entrada = new Scanner(System.in);
		
		System.out.println();
		String text = entrada.nextLine();
		
		while (text.equals("Sair")) {
			entrada.nextLine();
		}
		
		entrada.close();
		
	}
	
}
